 {
    "name": "Extension Disabler",
    "version": "0.5",
    "description": "Easily disable extensions",
    "permissions": ["storage","management"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
    "manifest_version": 2,
    "browser_action": {
      "default_icon": "images/on.png",
      "default_title": "Extension Disabler"
    }
}